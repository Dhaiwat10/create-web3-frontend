{
  "name": "create-web3-frontend",
  "version": "2.1.0",
  "description": "Initialize a React web3 frontend using Next.js, wagmi, ethers and TailwindCSS",
  "repository": "https://github.com/dhaiwat10/create-web3-frontend",
  "author": "Dhaiwat Pandya <dhaiwatpandya@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "react",
    "next",
    "next.js",
    "web3",
    "wagmi",
    "tailwind",
    "rainbowkit",
    "rainbow"
  ],
  "bin": {
    "create-web3-frontend": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublish": "pnpm release",
    "build": "ncc build ./index.ts -o ./dist/"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^18.15.11",
    "@types/prompts": "2.0.1",
    "@types/rimraf": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/validate-npm-package-name": "3.0.0",
    "@vercel/ncc": "0.36.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "got": "10.7.0",
    "prompts": "2.1.0",
    "rimraf": "3.0.0",
    "tar": "4.4.10",
    "typescript": "^4.9.5",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  },
  "engines": {
    "node": ">=12"
  }
}
